<head>
    <!-- CSS only -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
</head>
<body>
<div class="row">
    <div class="col-3"></div>
    <div class="col-6">
        <form action="/display" method="post" enctype="multipart/form-data">
            <h3 class="col-12">Viet Blog</h3>
            <div class="col-8">
                <h5 class="col-12">Title</h5>
                <input type="text" name="title" class="col-12" >
            </div>
            <div>
                <h5 class="col-12">Content</h5>
                <textarea name="content" id="" cols="30" rows="10" class="col-12"></textarea>
            </div>
            <button class="btn btn-danger" type="submit" style="margin-top: 20px; margin-left: 45%">Send</button>
        </form>
    </div>
    <div class="col-3"></div>
</div>

<!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
</body>



